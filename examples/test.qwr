extern printf :: (fmt: *u8, ..) s32;

print :: (v: int) {
    printf("%d\n", v);
}

print :: (v: *u8) {
    printf("%s\n", v);
}

Weekday :: enum {
    Monday,
    Tuesday,
    Wednesday,
    Thursday,
    Friday,
    Saturday,
    Sunday,
}

main :: () int {
    print(Weekday.Monday);
    print(Weekday.Tuesday);
    print(Weekday.Wednesday);
    print(Weekday.Friday);
    print(Weekday.Saturday);
    print(Weekday.Sunday);

    return 0;
}
