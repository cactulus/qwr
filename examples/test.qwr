extern printf :: (fmt: *u8, ..) s32;

print :: (v: int) {
    printf("%d\n", v);
}

print :: (v: *u8) {
    printf("%s\n", v);
}

sub :: (a: s32, b: s32) s32, s32 {
    return a - b, b - a;
}

main :: () int {
    a := 8;
    b := 5; 

    c, d := sub(a, b);
    e := sub(a, b);

    print(c);
    print(d);
    print(e);

    return 0;
}
