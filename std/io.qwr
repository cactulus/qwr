extern printf :: (fmt: string, ..) s32;

print :: (i: s64) {
    printf("%d\n", i);
}

print :: (s: string) {
    printf("%s\n", s);
}

print :: (p: *u8) {
    printf("%p\n", p);
}

print :: (c: char) {
    printf("%c\n", c);
}

print :: (f: f64) {
	printf("%f\n", f);
}

extern exit :: (status: int);
