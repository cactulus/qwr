min :: (a int, b int) int {
	if a < b
		return a;

	return b;
}

max :: (a int, b int) int {
	if a > b
		return a;
	
	return b;
}

degree_to_radian :: (deg f64) f64 {
    return deg * (PI / 180.0); 
}
